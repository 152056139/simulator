<?xml version="1.0" encoding="utf-8"?>
<conf>
    <!-- 开关 -->
    <switch>true</switch>
    <!-- 休眠时间 -->
    <sleeptime>10</sleeptime>
    <!-- 数据趋势的加速度（新陈代谢速率） -->
    <acceleration>0.015</acceleration>

    <!-- 低压 -->
    <value_low_pressure>92.34</value_low_pressure>
    <max_low_pressure>110</max_low_pressure>
    <min_low_pressure>90</min_low_pressure>
    <vectory_low_pressure>0</vectory_low_pressure>

    <!-- 高压 -->
    <value_high_pressure>124.43</value_high_pressure>
    <max_high_pressure>130</max_high_pressure>
    <min_high_pressure>110</min_high_pressure>
    <vectory_high_pressure>0</vectory_high_pressure>

    <!-- 心率 -->
    <value_heart_rate>62.74</value_heart_rate>
    <max_heart_rate>100</max_heart_rate>
    <min_heart_rate>60</min_heart_rate>
    <vectory_heart_rate>0</vectory_heart_rate>

    <!-- 呼吸频率 -->
    <value_breathe_rate>22.92</value_breathe_rate>
    <max_breathe_rate>25</max_breathe_rate>
    <min_breathe_rate>20</min_breathe_rate>
    <vectory_breathe_rate>0</vectory_breathe_rate>

    <!-- 体温 -->
    <value_body_temperature>38.11</value_body_temperature>
    <max_body_temperature>40</max_body_temperature>
    <min_body_temperature>36</min_body_temperature>
    <vectory_body_temperature>0</vectory_body_temperature>

    <!-- 车速 -->
    <value_car_rate>111.89</value_car_rate>
    <max_car_rate>120</max_car_rate>
    <min_car_rate>0</min_car_rate>
    <vectory_car_rate>0</vectory_car_rate>

    <!-- 车内温度 -->
    <value_car_temperature>26.36</value_car_temperature>
    <max_car_temperature>45</max_car_temperature>
    <min_car_temperature>25</min_car_temperature>
    <vectory_car_temperature>0</vectory_car_temperature>

    <!-- 车内饱和度 -->
    <value_oxygen>41.93</value_oxygen>
    <max_oxygen>45</max_oxygen>
    <min_oxygen>40</min_oxygen>
    <vectory_oxygen>0</vectory_oxygen>

    <!-- 车内湿度 -->
    <value_humidity>32.29</value_humidity>
    <max_humidity>35</max_humidity>
    <min_humidity>30</min_humidity>
    <vectory_humidity>0</vectory_humidity>
</conf>
